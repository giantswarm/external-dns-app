{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "annotationFilter": {
            "type": [
                "string",
                "null"
            ]
        },
        "automountServiceAccountToken": {
            "type": "boolean"
        },
        "baseDomain": {
            "type": "string"
        },
        "ciliumNetworkPolicy": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "clusterID": {
            "type": "string"
        },
        "commonLabels": {
            "type": "object"
        },
        "crd": {
            "type": "object",
            "properties": {
                "install": {
                    "type": "boolean"
                }
            }
        },
        "deploymentAnnotations": {
            "type": "object"
        },
        "deploymentStrategy": {
            "type": "object",
            "properties": {
                "type": {
                    "default": "Recreate",
                    "type": "string",
                    "enum": [
                        "Recreate",
                        "RollingUpdate"
                    ]
                }
            },
            "additionalProperties": true
        },
        "dnsConfig": {
            "type": [
                "object",
                "null"
            ]
        },
        "dnsPolicy": {
            "type": [
                "string",
                "null"
            ]
        },
        "domainFilters": {
            "type": "array"
        },
        "e2e": {
            "type": "boolean"
        },
        "enabled": {
            "description": "No effect - reserved for use in sub-charting",
            "type": [
                "boolean",
                "null"
            ]
        },
        "env": {
            "type": "array"
        },
        "excludeDomains": {
            "type": "array"
        },
        "extraArgs": {
            "type": [
                "array",
                "null",
                "object"
            ],
            "uniqueItems": true,
            "items": {
                "type": "string"
            }
        },
        "extraContainers": {
            "type": "array"
        },
        "extraVolumeMounts": {
            "type": "array"
        },
        "extraVolumes": {
            "type": "array"
        },
        "fullnameOverride": {
            "type": [
                "string",
                "null"
            ]
        },
        "gatewayNamespace": {
            "type": [
                "string",
                "null"
            ]
        },
        "global": {
            "type": "object",
            "properties": {
                "imagePullSecrets": {
                    "type": "array",
                    "items": {
                        "type": "object"
                    }
                }
            }
        },
        "hostNetwork": {
            "type": "boolean"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string",
                    "enum": [
                        "IfNotPresent",
                        "Always"
                    ]
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "additionalProperties": false
        },
        "imagePullSecrets": {
            "type": "array",
            "items": {
                "type": "object"
            }
        },
        "initContainers": {
            "type": "array"
        },
        "interval": {
            "type": "string"
        },
        "kubectlApplyJob": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "files": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "labelFilter": {
            "type": [
                "string",
                "null"
            ]
        },
        "livenessProbe": {
            "type": "object",
            "properties": {
                "failureThreshold": {
                    "type": "integer"
                },
                "httpGet": {
                    "type": "object",
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "port": {
                            "type": "string"
                        }
                    }
                },
                "initialDelaySeconds": {
                    "type": "integer"
                },
                "periodSeconds": {
                    "type": "integer"
                },
                "successThreshold": {
                    "type": "integer"
                },
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "logFormat": {
            "default": "text",
            "type": "string",
            "enum": [
                "text",
                "json"
            ]
        },
        "logLevel": {
            "default": "info",
            "type": "string",
            "enum": [
                "panic",
                "debug",
                "info",
                "warning",
                "error",
                "fatal"
            ]
        },
        "managedRecordTypes": {
            "type": [
                "array",
                "null"
            ],
            "uniqueItems": true,
            "items": {
                "type": "string"
            }
        },
        "minEventSyncInterval": {
            "type": "string"
        },
        "nameOverride": {
            "type": [
                "string",
                "null"
            ]
        },
        "namespaceFilter": {
            "type": "string"
        },
        "namespaced": {
            "type": "boolean"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object",
            "properties": {
                "cluster-autoscaler.kubernetes.io/safe-to-evict": {
                    "type": "string"
                },
                "kubectl.kubernetes.io/default-container": {
                    "type": "string"
                }
            }
        },
        "podLabels": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object",
            "properties": {
                "fsGroup": {
                    "type": "integer"
                },
                "runAsNonRoot": {
                    "type": "boolean"
                },
                "seccompProfile": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "policy": {
            "default": "upsert-only",
            "type": "string",
            "enum": [
                "create-only",
                "sync",
                "upsert-only"
            ]
        },
        "priorityClassName": {
            "type": [
                "string",
                "null"
            ]
        },
        "provider": {
            "type": [
                "object",
                "string"
            ],
            "properties": {
                "name": {
                    "type": "string"
                },
                "webhook": {
                    "type": "object",
                    "properties": {
                        "args": {
                            "type": "array"
                        },
                        "env": {
                            "type": "array"
                        },
                        "extraVolumeMounts": {
                            "type": "array"
                        },
                        "image": {
                            "type": "object",
                            "properties": {
                                "pullPolicy": {
                                    "type": "string"
                                },
                                "repository": {
                                    "type": [
                                        "string",
                                        "null"
                                    ]
                                },
                                "tag": {
                                    "type": [
                                        "string",
                                        "null"
                                    ]
                                }
                            }
                        },
                        "livenessProbe": {
                            "type": "object",
                            "properties": {
                                "failureThreshold": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                },
                                "httpGet": {
                                    "type": "object",
                                    "properties": {
                                        "path": {
                                            "type": [
                                                "string",
                                                "null"
                                            ]
                                        },
                                        "port": {
                                            "default": "string",
                                            "type": [
                                                "integer",
                                                "string"
                                            ]
                                        }
                                    }
                                },
                                "initialDelaySeconds": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                },
                                "periodSeconds": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                },
                                "successThreshold": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                },
                                "timeoutSeconds": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                }
                            }
                        },
                        "readinessProbe": {
                            "type": "object",
                            "properties": {
                                "failureThreshold": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                },
                                "httpGet": {
                                    "type": "object",
                                    "properties": {
                                        "path": {
                                            "type": [
                                                "string",
                                                "null"
                                            ]
                                        },
                                        "port": {
                                            "default": "string",
                                            "type": [
                                                "integer",
                                                "string"
                                            ]
                                        }
                                    }
                                },
                                "initialDelaySeconds": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                },
                                "periodSeconds": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                },
                                "successThreshold": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                },
                                "timeoutSeconds": {
                                    "type": [
                                        "integer",
                                        "null"
                                    ]
                                }
                            }
                        },
                        "resources": {
                            "type": "object"
                        },
                        "securityContext": {
                            "type": "object"
                        },
                        "service": {
                            "type": "object",
                            "properties": {
                                "port": {
                                    "type": "integer"
                                }
                            }
                        },
                        "serviceMonitor": {
                            "type": "object",
                            "properties": {
                                "bearerTokenFile": {
                                    "type": "null"
                                },
                                "interval": {
                                    "type": "null"
                                },
                                "metricRelabelings": {
                                    "type": "array"
                                },
                                "relabelings": {
                                    "type": "array"
                                },
                                "scheme": {
                                    "type": "null"
                                },
                                "scrapeTimeout": {
                                    "type": "null"
                                },
                                "tlsConfig": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "rbac": {
            "type": "object",
            "properties": {
                "additionalPermissions": {
                    "type": "array"
                },
                "create": {
                    "type": "boolean"
                }
            },
            "additionalProperties": true
        },
        "readinessProbe": {
            "type": "object",
            "properties": {
                "failureThreshold": {
                    "type": "integer"
                },
                "httpGet": {
                    "type": "object",
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "port": {
                            "type": "string"
                        }
                    }
                },
                "initialDelaySeconds": {
                    "type": "integer"
                },
                "periodSeconds": {
                    "type": "integer"
                },
                "successThreshold": {
                    "type": "integer"
                },
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "registry": {
            "default": "txt",
            "type": "string",
            "enum": [
                "txt",
                "aws-sd",
                "dynamodb",
                "noop"
            ]
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "revisionHistoryLimit": {
            "type": [
                "integer",
                "null"
            ],
            "minimum": 0
        },
        "secretConfiguration": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "mountPath": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "subPath": {
                    "type": [
                        "string",
                        "null"
                    ]
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "allowPrivilegeEscalation": {
                    "type": "boolean"
                },
                "capabilities": {
                    "type": "object",
                    "properties": {
                        "drop": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                },
                "privileged": {
                    "type": "boolean"
                },
                "readOnlyRootFilesystem": {
                    "type": "boolean"
                },
                "runAsGroup": {
                    "type": "integer"
                },
                "runAsNonRoot": {
                    "type": "boolean"
                },
                "runAsUser": {
                    "type": "integer"
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "ipFamilies": {
                    "type": [
                        "array",
                        "null"
                    ],
                    "maxItems": 2,
                    "minItems": 0,
                    "uniqueItems": true,
                    "items": {
                        "type": "string",
                        "enum": [
                            "IPv4",
                            "IPv6"
                        ]
                    }
                },
                "ipFamilyPolicy": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "enum": [
                        "SingleStack",
                        "PreferDualStack",
                        "RequireDualStack",
                        null
                    ]
                },
                "port": {
                    "default": 7979,
                    "type": "integer",
                    "minimum": 0
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "automountServiceAccountToken": {
                    "type": "boolean"
                },
                "create": {
                    "type": "boolean"
                },
                "labels": {
                    "type": "object"
                },
                "name": {
                    "type": [
                        "string",
                        "null"
                    ]
                }
            }
        },
        "serviceMonitor": {
            "type": "object",
            "properties": {
                "additionalLabels": {
                    "type": "object"
                },
                "annotations": {
                    "type": "object"
                },
                "bearerTokenFile": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "enabled": {
                    "type": "boolean"
                },
                "interval": {
                    "type": "string"
                },
                "metricRelabelings": {
                    "type": "array"
                },
                "namespace": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "relabelings": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "action": {
                                "type": "string"
                            },
                            "regex": {
                                "type": "string"
                            },
                            "replacement": {
                                "type": "string"
                            },
                            "separator": {
                                "type": "string"
                            },
                            "sourceLabels": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "targetLabel": {
                                "type": "string"
                            }
                        }
                    }
                },
                "scheme": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "scrapeTimeout": {
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "targetLabels": {
                    "type": "array"
                },
                "tlsConfig": {
                    "type": "object"
                }
            }
        },
        "serviceType": {
            "type": "string"
        },
        "shareProcessNamespace": {
            "type": "boolean"
        },
        "sources": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "terminationGracePeriodSeconds": {
            "type": [
                "integer",
                "null"
            ]
        },
        "tolerations": {
            "type": "array"
        },
        "topologySpreadConstraints": {
            "type": "array"
        },
        "triggerLoopOnEvent": {
            "type": "boolean"
        },
        "txtOwnerId": {
            "type": [
                "string",
                "null"
            ]
        },
        "txtPrefix": {
            "type": [
                "string",
                "null"
            ]
        },
        "txtSuffix": {
            "type": [
                "string",
                "null"
            ]
        }
    }
}
